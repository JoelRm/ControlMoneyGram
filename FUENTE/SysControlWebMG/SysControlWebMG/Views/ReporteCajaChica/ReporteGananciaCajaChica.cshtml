<script src="~/Content/MoneyGram/JavaScript/jquery.dataTables.min.js"></script>
<script src="~/Content/MoneyGram/JavaScript/dataTables.bootstrap.min.js"></script>

@{
    ViewBag.Title = "Index";
}
@using Entidades
@{
    List<UsuariosCLS> listaUsuarios = (List<UsuariosCLS>)ViewBag.listaUsuarios;
}
<div class="container-fluid">
    <div class="d-sm-flex align-items-center justify-content-between mb-4">
        <h1 class="h3 mb-0 text-gray-800">Reporte Ganancia Caja Chica</h1>
    </div>
    <div class="form-row">
        <div class="col-md-2">
            <div class="form-group">
                <label>Fecha: </label>
                <input type="date" id="fInicio" class="form-control" />
            </div>
        </div>
        <div class="col-lg-2">
            <button type="button" id="btnBuscarGananciaCajaChica" class="btn btn-danger btn-lg px-3"><i class="fa fa-search" aria-hidden="true"></i> Buscar </button>
        </div>
    </div>
    <br>
    <div class="panel-body">
        <div class="form-horizontal row">
            <div class="col-sm-12">
                <div class="form-group">
                    <div class="col-sm-12">
                        <div id="div_tbListaGananciaCajaChica" style="height: 100%;">
                            <table id="tbListaGananciaCajaChica" class="table table-bordered table-hover" style="width:1200px">
                                <thead>
                                    <tr>
                                        <th data-width="10px">Tipo Cambio Ref.</th>
                                        <th data-width="10px">Caja Chica Inicio</th>
                                        <th data-width="10px">Caja Chica Final</th>
                                        <th data-width="10px">Ingresos</th>
                                        <th data-width="10px">Envíos</th>
                                        <th data-width="10px">Pagos</th>
                                        <th data-width="10px">Gastos</th>
                                        <th data-width="10px">GananciaCajaChica</th>
                                    </tr>
                                </thead>
                                <tbody id="bodyTbListaGananciaCajaChica"></tbody>
                            </table>
                        </div>
                        <div id="div_tbListaGananciaCajaChica_pagination" style="padding-top: 5px;"></div>
                    </div>
                </div>
            </div>
        </div>
    </div>
</div>
<script src="~/Scripts/Pages/ReportesCajaChica/ReporteGananciaCajaChica.js"></script>